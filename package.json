{
  "name": "roger_dashboard",
  "productName": "RoGeR Dashboard",
  "version": "0.2.3",
  "private": true,
  "main": ".vite/build/main.js",
  "author": {
    "name": "Max Schmit",
    "email": "max.schmit@hydrology.uni-freiburg.de"
  },
  "license": "MIT",
  "description": "This is a small dashboard to visualize RoGeR model results",
  "icon": "public/favicon.ico",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hydrology-IFH/roger_dashboard"
  },
  "scripts": {
    "dev": "electron-forge start",
    "build": "electron-forge make",
    "publish": "electron-forge publish",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "build": {
    "appId": "com.electron.roger_dashboard",
    "productName": "RoGeR Dashboard",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "index.html",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "portable",
      "icon": "public/favicon.ico"
    }
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^3.4.1",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "colormap": "^2.3.2",
    "csv-parse": "^5.6.0",
    "electron-store": "^10.1.0",
    "geotiff": "^2.1.3",
    "grid-layout-plus": "^1.1.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "ol": "^10.5.0",
    "ol-ext": "^4.0.32",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^3.2.3",
    "plotly": "^1.0.6",
    "plotly.js-dist": "^2.35.3",
    "popper.js": "^1.16.1",
    "proj4": "^2.17.0",
    "uuid": "^9.0.1",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.16",
    "vue3-plotly": "^0.0.7",
    "vuetify": "^3.8.9"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.11.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "electron": "^36.4.0",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "vite": "^5.4.19",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-plugin-vue-devtools": "^7.7.7"
  }
}