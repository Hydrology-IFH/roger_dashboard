{
  "name": "roger_dashboard",
  "productName": "RoGeR Dashboard",
  "version": "0.1.1",
  "private": true,
  "main": ".vite/build/main.js",
  "author": {
    "name": "Max Schmit",
    "email": "max.schmit@hydrology.uni-freiburg.de"
  },
  "license": "MIT",
  "description": "A dashboard for ROGER model results",
  "icon": "public/favicon.ico",
  "scripts": {
    "dev": "electron-forge start",
    "build": "electron-forge make",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "build": {
    "appId": "com.electron.roger_dashboard",
    "productName": "RoGeR Dashboard",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "index.html",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "portable",
      "icon": "public/favicon.ico"
    }
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^3.2.1",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "colormap": "^2.3.2",
    "csv-parse": "^5.5.5",
    "electron-store": "^10.0.0",
    "geotiff": "^2.1.3",
    "grid-layout-plus": "^1.0.5",
    "jquery": "^3.7.1",
    "ol": "^9.1.0",
    "ol-ext": "^4.0.18",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "plotly": "^1.0.6",
    "plotly.js-dist": "^2.30.1",
    "popper.js": "^1.16.1",
    "proj4": "^2.11.0",
    "uuid": "^9.0.1",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.21",
    "vue3-plotly": "^0.0.7",
    "vuetify": "^3.6.8"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^8.0.0",
    "electron": "^30.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "sass": "^1.75.0",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-plugin-vue-devtools": "^7.0.18"
  }
}
